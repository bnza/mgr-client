{
  "name": "medgr-client",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "compare:query-params": "npx tsx bin/compare-query-params.ts",
    "dev": "nuxt dev",
    "eslint:check": "eslint --ext .js,.ts,.tsx,.vue,.mjs --fix-dry-run",
    "generate": "nuxt generate",
    "generate:openapi": "node bin/generate-openapi-types.mjs && prettier --write types/openapi.d.ts",
    "generate:api-resources": "node bin/generate-api-resources-types.mjs && prettier --write app/utils/consts/resources.ts",
    "generate:all": "pnpm generate:openapi && pnpm generate:api-resources",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,vue,css,scss,json,md}\" --ignore-path .prettierignore",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,vue,css,scss,json,md}\" --ignore-path .prettierignore",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test:unit": "vitest",
    "test:unit:run": "vitest run",
    "test:unit:coverage": "vitest run --coverage",
    "test:unit:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "typecheck": "vue-tsc --noEmit -p tsconfig.json --composite false",
    "typecheck:tests": "vue-tsc --noEmit -p tests/tsconfig.json --composite false",
    "typecheck:all": "pnpm typecheck && pnpm typecheck:tests"
  },
  "dependencies": {
    "@nuxtjs/google-fonts": "^3.2.0",
    "@pinia/colada": "^0.17.8",
    "@pinia/colada-nuxt": "^0.2.4",
    "@pinia/colada-plugin-retry": "^0.1.2",
    "@pinia/nuxt": "^0.11.3",
    "@regle/core": "^1.10.1",
    "@regle/rules": "^1.10.1",
    "@sidebase/nuxt-auth": "^1.1.0",
    "@types/jsonld": "^1.5.15",
    "deep-object-diff": "^1.1.9",
    "dotenv": "^17.2.3",
    "js-sha256": "^0.11.1",
    "nuxt": "^4.2.1",
    "ol": "^10.7.0",
    "pinia": "^3.0.4",
    "qs": "^6.14.0",
    "vue": "latest",
    "vue-router": "latest",
    "vue3-openlayers": "^11.6.2",
    "vuetify": "^3.10.11",
    "vuetify-nuxt-module": "^0.18.8"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.10.0",
    "@nuxt/test-utils": "^3.20.1",
    "@pinia/colada-devtools": "^0.1.8",
    "@pinia/testing": "^1.0.3",
    "@playwright/test": "^1.56.1",
    "@types/geojson": "^7946.0.16",
    "@types/node": "^22.19.1",
    "@types/qs": "^6.14.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.39.1",
    "jsdom": "^26.1.0",
    "openapi-types": "^12.1.3",
    "openapi-typescript": "^7.10.1",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12"
  },
  "overrides": {
    "vue": "latest"
  }
}
